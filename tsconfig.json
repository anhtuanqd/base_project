{
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./tsc",
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "allowJs": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules"]
}
